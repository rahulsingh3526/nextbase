{
  "ignorePatterns": [
    "**/node_modules/**",
    "**/*.tests.{js,jsx,ts,tsx}",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "**/tests/**",
    "**/__tests__/**",
    "**/*.d.ts",
    "**/public/**",
    "**/styles/**",
    "next.config.js"
  ],
  "entry": [
    "./src/pages/**/*.{js,jsx,ts,tsx}",
    "./src/app/**/{route,layout,page,loading,error,not-found,default}.{js,jsx,ts,tsx}",
    "src/middleware.{js,jsx,ts,tsx}",
    "next.config.{js,jsx,ts,tsx,mjs}",
    "src/mdx-components.{js,jsx,ts,tsx}"
  ],
  "ignoreUnimported": [],
  "ignoreUnused": ["next", "react", "react-dom"],
  "ignoreUnresolved": [],
  "respectGitignore": true
}
